{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "CourseraLens": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "MyBGList": "Information"
      }
    }
  },
  "Console": {
    "FormatterOptions": {
      "SingleLine": true,
      "TimestampFormat": "HH:mm:ss ",
      "UseUtcTimestamp": true
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "*",
  "UseDeveloperExceptionPage": false,
  "JWT": {
    "Issuer": "CourseraLensAPI",
    "Audience": "CourseraLensClient",
    "ExpireMinutes": 60
  }
}
